<UserControl x:Class="Win_Design.Controls.Design_Controls.Demo_Controls.Button"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Win_Design.Controls.Design_Controls.Demo_Controls"
             mc:Ignorable="d" Width="300" Height="140">
    <Border Background="White" CornerRadius="9,9,9,9">
        <Border BorderBrush="#FFB1B1B1" BorderThickness="1" Margin="2,2,2,2" Background="Black" CornerRadius="8,8,8,8">
            <Label Content="Button" Foreground="#FFC3C3C3" x:Name="Texts" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="-3,-1,-1,-1"/>
        </Border>
    </Border>
</UserControl>
